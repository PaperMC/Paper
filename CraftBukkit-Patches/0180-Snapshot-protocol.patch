+        pMap.put( id, packet );
+        Field map = EnumProtocol.class.getDeclaredField( "f" );
+        map.setAccessible( true );
+        Map<Class<? extends Packet>, EnumProtocol> protocolMap = (Map<Class<? extends Packet>, EnumProtocol>) map.get( null );
