+    // Paper start - optimise getPlayerByUUID
+    @Nullable
+    @Override
+    public Player getPlayerByUUID(UUID uuid) {
+        Entity player = this.entitiesByUuid.get(uuid);
+        return (player instanceof Player) ? (Player)player : null;
+    }
