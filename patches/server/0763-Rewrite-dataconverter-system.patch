+
+        chunkBlocks.remove("minecraft:air");
+        final boolean chunkNotEmpty = !chunkBlocks.isEmpty();
+        chunkBlocks.removeAll(BLOCKS_BEFORE_FEATURE_STATUS);
+        final boolean chunkFeatureStatus = !chunkBlocks.isEmpty();
