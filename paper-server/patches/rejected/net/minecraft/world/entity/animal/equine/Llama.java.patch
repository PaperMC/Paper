@@ -191,12 +192,12 @@ public class Llama extends AbstractChestedHorse implements RangedAttackMob {
             f = 10.0F;
             if (this.isTamed() && this.getAge() == 0 && this.canFallInLove()) {
                 flag = true;
-                this.setInLove(player);
